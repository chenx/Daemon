<?xml version="1.0" encoding="utf-8" ?>
<configuration>
<appSettings>
    <add key="UseLog" value="True" />
    <add key="LogDir" value="log/" />
    <!--timeout unit: millisecond-->
    <add key="Timeout" value="5" />
    <!--targets are separated by comma ","-->
    <add key="Targets" value="localhost:9092" />
</appSettings>
</configuration>
